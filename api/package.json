    "@babel/preset-env": "^7.4.4",
    "babel-loader": "^8.0.5",
    "dotenv": "^8.0.0",
    "jest": "^24.8.0",
    "mock-express-request": "^0.2.2",
    "mock-express-response": "^0.2.2",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "bufferutil": "^4.0.1",
    "express": "^4.16.4",
    "graphile-utils": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^1.19.1",
    "postgraphile": "^4.4.0",
    "ramda": "^0.26.1",
    "redis": "^2.8.0",
